<%-include('header')-%>
    <div class="container mt-5">
        <form action="/add-new" method="post">
            <div class="form-group row">
                <label class="col-sm-2 col-form-label" for="rollNumber">Roll Number</label>
                <div class="col-sm-10">
                    <input class="form-control" type="number" id="rollNumber" name="rollNumber" required>
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label" for="name">Name</label>
                <div class="col-sm-10">
                    <input class="form-control" type="text" id="name" name="name" required>
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label" for="date_of_birth">Date Of Birth</label>
                <div class="col-sm-10">
                    <input class="form-control" type="date" id="date_of_birth" name="date_of_birth" required>
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label" for="score">Score</label>
                <div class="col-sm-10">
                    <input class="form-control" type="number" id="score" name="score" required>
                </div>
            </div>
            <button class="btn btn-primary" type="submit">Save</button>
            <button class="btn btn-danger" type="reset">Clear</button>
        </form>
        <form action="/addresult"><button class="btn btn-secondary" type="submit">Back</button></form>
    </div>


    <%-include('footer')-%>