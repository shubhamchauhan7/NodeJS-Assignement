<%-include('header')-%>
    <div class="container mt-5">
        <h1>Find Result</h1>
        <form action="/find" method="POST">
            <div class="form-group row">
                <label class="col-sm-2 col-form-label" for="rollNumber">Roll Number</label>
                <div class="col-sm-10">
                    <input class="form-control" type="number" id="rollNumber" name="rollNumber" required>
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label" for="name">Name</label>
                <div class="col-sm-10">
                    <input class="form-control" type="text" id="name" name="name" required>
                </div>
            </div>
            <button class="btn btn-info" type="submit">Submit</button>
    <button class="btn btn-danger" type="reset">Clear</button>
        </form>
        <form action="/addresult"><button class="btn btn-secondary" type="submit">Back</button></form>
    </div>

    <%-include('footer')-%>